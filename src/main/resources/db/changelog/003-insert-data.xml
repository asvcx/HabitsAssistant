                <column name="title" value="Running"/>
                <column name="description" value="For health"/>
                <column name="period" valueNumeric="1"/>
                <column name="start_date" value="2024-10-17 14:30:00"/>
            </insert>

    </changeSet>
</databaseChangeLog>